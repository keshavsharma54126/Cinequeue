{
  "name": "cinequeue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tsc": "tsc --noEmit",
    "format": "prettier \"**/*.{ts,tsx,js,json}\" --ignore-path .gitignore ",
    "format:check": "prettier \"**/*.{ts,tsx,js,json}\"  --check --ignore-path .gitignore",
    "format:write": "prettier \"**/*.{ts,tsx,js,json}\"  --write --ignore-path .gitignore",
    "validate": "npm run format:write && npm run lint && npm run tsc && npm run build"
  },
  "dependencies": {
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
